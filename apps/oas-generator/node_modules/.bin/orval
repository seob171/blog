#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/dist/bin/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/dist/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/dist/bin/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/dist/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules/orval/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/orval@7.0.1_openapi-types@12.1.3_typescript@5.5.4/node_modules:/Users/yuseobshim/WebstormProjects/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../orval/dist/bin/orval.js" "$@"
else
  exec node  "$basedir/../orval/dist/bin/orval.js" "$@"
fi
